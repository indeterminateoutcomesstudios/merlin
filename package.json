{
  "name": "deeMemory",
  "productName": "DeeMemory",
  "version": "1.0.0",
  "description": "A tool to help a Dungeon Master",
  "license": "MIT",
  "repository": "user/repo",
  "author": {
    "name": "Marco Zanini",
    "email": "chetto87+deeMemory@gmail.com",
    "url": "http://www.marcozanini.it"
  },
  "main": "app/background.js",
  "build": {
    "appId": "deeMemory.id",
    "linux": {
      "target": "pacman"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "scripts": {
    "postinstall": "install-app-deps",
    "unit": "jest --verbose",
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "pack": "build --dir",
    "dist": "build",
    "distWin": "build --win",
    "dev": "webpack --config build/webpack.config.js --progress --colors --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "dev:hot": "webpack-dev-server --config build/webpack.test.config.js --hot --inline --progress --colors --watch --content-base ./"
  },
  "jest": {
    "setupFiles": [
      "./test/testsetup.js"
    ],
    "automock": false
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "autoprefixer": "^9.1.3",
    "bootstrap": "^4.1.3",
    "chance": "^1.0.13",
    "electron-debug": "^2.0.0",
    "electron-settings": "^3.1.4",
    "jquery": "^3.2.1",
    "json2csv": "^4.2.1",
    "pev": "^1.0.0",
    "popper.js": "^1.12.9",
    "postcss-loader": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "url-loader": "^1.1.1"
  },
  "devDependencies": {
    "b": "^2.0.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "devtron": "^1.1.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-packager": "^12.1.1",
    "electron-react-devtools": "^0.5.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.5.0",
    "jsdom": "^12.0.0",
    "mocha": "^5.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "readline": "^1.3.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.1",
    "xo": "^0.22.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
