{
  "name": "deeMemory",
  "productName": "DeeMemory",
  "version": "1.0.0",
  "description": "A tool to help a Dungeon Master",
  "license": "MIT",
  "repository": "user/repo",
  "author": {
    "name": "Marco Zanini",
    "email": "chetto87+deeMemory@gmail.com",
    "url": "http://www.marcozanini.it"
  },
  "main": "app/background.js",
  "build": {
    "appId": "deeMemory.id",
    "linux": {
      "target": "pacman"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "unit": "jest --verbose",
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "pack": "build --dir",
    "dist": "build",
    "distWin": "build --win",
    "dev": "webpack --config build/webpack.config.js --progress --colors --watch --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "dev:hot": "webpack-dev-server --config build/webpack.test.config.js --hot --inline --progress --colors --watch --content-base ./"
  },
  "jest": {
    "setupFiles": [
      "./test/testsetup.js"
    ],
    "automock": false
  },
  "dependencies": {
    "@material-ui/core": "1.5.1",
    "autoprefixer": "9.1.3",
    "bootstrap": "4.1.3",
    "chance": "1.0.16",
    "electron-debug": "2.0.0",
    "electron-settings": "3.2.0",
    "jquery": "3.3.1",
    "json2csv": "4.2.1",
    "pev": "1.0.0",
    "popper.js": "1.14.4",
    "postcss-loader": "3.0.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "url-loader": "1.1.1"
  },
  "devDependencies": {
    "b": "2.0.1",
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "css-loader": "1.0.0",
    "devtron": "1.4.0",
    "electron": "2.0.8",
    "electron-builder": "20.28.2",
    "electron-packager": "12.1.1",
    "electron-react-devtools": "0.5.3",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.0",
    "jest": "23.5.0",
    "jsdom": "12.0.0",
    "mocha": "5.2.0",
    "react-test-renderer": "16.4.2",
    "readline": "1.3.0",
    "style-loader": "0.22.1",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "4.1.4",
    "xo": "0.22.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
